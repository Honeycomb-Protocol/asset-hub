{
  "name": "honeycomb",
  "version": "1.0.0",
  "private": false,
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "bin": {
    "honeycomb": "./dist/bin/index.ts"
  },
  "scripts": {
    "build:program": "anchor build",
    "build:sdk": "rm -rf dist && solita && tsc",
    "build": "yarn build:program & yarn build:sdk",
    "deploy": "anchor deploy",
    "honeycomb:dev": "ts-node src/bin/index.ts",
    "build-deploy": "yarn build && yarn deploy",
    "assembler": "ts-node ./scripts/index.ts assembler",
    "assetmanager": "ts-node ./scripts/index.ts assetmanager",
    "postinstall": "node scripts/postinstall.js"
  },
  "dependencies": {
    "@metaplex-foundation/js": "^0.17.12",
    "@project-serum/anchor": "^0.26.0",
    "@solana/spl-token": "^0.3.7",
    "yargs": "^17.6.2",
    "@types/yargs": "^17.0.20"
  },
  "devDependencies": {
    "@metaplex-foundation/solita": "^0.19.3",
    "@types/bn.js": "^5.1.0",
    "prettier": "^2.6.2",
    "typescript": "^4.3.5"
  }
}
